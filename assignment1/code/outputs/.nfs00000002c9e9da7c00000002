rm -f  build/matmult.o
rm -rf lib build Makefile.dep outputs figures data
rm -f  build/matmult.o
gcc -MM -g -fPIC  -Wall src/matmult.c > Makefile.dep
gcc -g -fPIC  -Wall -c src/matmult.c -o build/matmult.o
gcc -g -fPIC  -Wall -o lib/libmatmult_default.so -shared   build/matmult.o -L/usr/lib64/atlas -lsatlas
rm -f  build/matmult.o
gcc -MM -g -O -fPIC  -Wall src/matmult.c > Makefile.dep
gcc -g -O -fPIC  -Wall -c src/matmult.c -o build/matmult.o
gcc -g -O -fPIC  -Wall -o lib/libmatmult_O.so -shared   build/matmult.o -L/usr/lib64/atlas -lsatlas
rm -f  build/matmult.o
gcc -MM -g -O2 -fPIC  -Wall src/matmult.c > Makefile.dep
gcc -g -O2 -fPIC  -Wall -c src/matmult.c -o build/matmult.o
gcc -g -O2 -fPIC  -Wall -o lib/libmatmult_O2.so -shared   build/matmult.o -L/usr/lib64/atlas -lsatlas
rm -f  build/matmult.o
gcc -MM -g -O3 -fPIC  -Wall src/matmult.c > Makefile.dep
gcc -g -O3 -fPIC  -Wall -c src/matmult.c -o build/matmult.o
gcc -g -O3 -fPIC  -Wall -o lib/libmatmult_O3.so -shared   build/matmult.o -L/usr/lib64/atlas -lsatlas
rm -f  build/matmult.o
gcc -MM -g -Ofast -fPIC  -Wall src/matmult.c > Makefile.dep
gcc -g -Ofast -fPIC  -Wall -c src/matmult.c -o build/matmult.o
gcc -g -Ofast -fPIC  -Wall -o lib/libmatmult_Ofast.so -shared   build/matmult.o -L/usr/lib64/atlas -lsatlas
rm -f  build/matmult.o
